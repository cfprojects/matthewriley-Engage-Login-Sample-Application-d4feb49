ame" type="string" required="false" default="" />		<cfargument name="formattedName" type="string" required="false" default="" />		<cfargument name="givenName" type="string" required="false" default="" />		<cfargument name="preferredUsername" type="string" required="false" default="" />		<cfargument name="providerName" type="string" required="false" default="" />		<cfargument name="url" type="string" required="false" default="" />		<cfargument name="verifiedEmail" type="string" required="false" default="" />		<cfargument name="systemCreationDate" type="string" required="false" default="" />		<cfargument name="lastLogin" type="string" required="false" default="" />				<cfscript>			setprofileID(arguments.profileID);			setdisplayName(arguments.displayName);			setemail(arguments.email);			setuserId(arguments.userId);			setidentifier(arguments.identifier);			setfamilyName(arguments.familyName);			setformattedName(arguments.formattedName);			setgivenName(arguments.givenName);			setpreferredUsername(arguments.preferredUsername);			setproviderName(arguments.providerName);			seturl(arguments.url);			setverifiedEmail(arguments.verifiedEmail);			setsystemCreationDate(arguments.systemCreationDate);			setlastLogin(arguments.lastLogin);			return this;		</cfscript> 	</cffunction>		<cffunction name="setprofileID" access="public" returntype="void" output="false">		<cfargument name="profileID" type="string" required="true" />		<cfset variables.instance.profileID = Trim(arguments.profileID) />	</cffunction>	<cffunction name="getprofileID" access="public" returntype="string" output="false">		<cfreturn variables.instance.profileID />	</cffunction>	<cffunction name="setdisplayName" access="public" returntype="void" output="false">		<cfargument name="displayName" type="string" required="true" />		<cfset variables.instance.displayName = Trim(arguments.displayName) />	</cffunction>	<cffunction name="getdisplayName" access="public" returntype="string" output="false">		<cfreturn variables.instance.displayName />	</cffunction>	<cffunction name="setemail" access="public" returntype="void" output="false">		<cfargument name="email" type="string" required="true" />		<cfset variables.instance.email = Trim(arguments.email) />	</cffunction>	<cffunction name="getemail" access="public" returntype="string" output="false">		<cfreturn variables.instance.email />	</cffunction>	<cffunction name="setuserId" access="public" returntype="void" output="false">		<cfargument name="userId" type="string" required="true" />		<cfset variables.instance.userId = Trim(arguments.userId) />	</cffunction>	<cffunction name="getuserId" access="public" returntype="string" output="false">		<cfreturn variables.instance.userId />	</cffunction>	<cffunction name="setidentifier" access="public" returntype="void" output="false">		<cfargument name="identifier" type="string" required="true" />		<cfset variables.instance.identifier = Trim(arguments.identifier) />	</cffunction>	<cffunction name="getidentifier" access="public" returntype="string" output="false">		<cfreturn variables.instance.identifier />	</cffunction>	<cffunction name="setfamilyName" access="public" returntype="void" output="false">		<cfargument name="familyName" type="string" required="true" />		<cfset variables.instance.familyName = Trim(arguments.familyName) />	</cffunction>	<cffunction name="getfamilyName" access="public" returntype="string" output="false">		<cfreturn variables.instance.familyName />	</cffunction>	<cffunction name="setformattedName" access="public" returntype="void" output="false">		<cfargument name="formattedName" type="string" required="true" />		<cfset variables.instance.formattedName = Trim(arguments.formattedName) />	</cffunction>	<cffunction name="getformattedName" access="public" returntype="string" output="false">		<cfreturn variables.instance.formattedName />	</cffunction>	<cffunction name="setgivenName" access="public" returntype="void" output="false">		<cfargument name="givenName" type="string" required="true" />		<cfset variables.instance.givenName = Trim(arguments.givenName) />	</cffunction>	<cffunction name="getgivenName" access="public" returntype="string" output="false">		<cfreturn variables.instance.givenName />	</cffunction>	<cffunction name="setpreferredUsername" access="public" returntype="void" output="false">		<cfargument name="preferredUsername" type="string" required="true" />		<cfset variables.instance.preferredUsername = Trim(arguments.preferredUsername) />	</cffunction>	<cffunction name="getpreferredUsername" access="public" returntype="string" output="false">		<cfreturn variables.instance.preferredUsername />	</cffunction>	<cffunction name="setproviderName" access="public" returntype="void" output="false">		<cfargument name="providerName" type="string" required="true" />		<cfset variables.instance.providerName = Trim(arguments.providerName) />	</cffunction>	<cffunction name="getproviderName" access="public" returntype="string" output="false">		<cfreturn variables.instance.providerName />	</cffunction>	<cffunction name="seturl" access="public" returntype="void" output="false">		<cfargument name="url" type="string" required="true" />		<cfset variables.instance.url = Trim(arguments.url) />	</cffunction>	<cffunction name="geturl" access="public" returntype="string" output="false">		<cfreturn variables.instance.url />	</cffunction>	<cffunction name="setverifiedEmail" access="public" returntype="void" output="false">		<cfargument name="verifiedEmail" type="string" required="true" />		<cfset variables.instance.verifiedEmail = Trim(arguments.verifiedEmail) />	</cffunction>	<cffunction name="getverifiedEmail" access="public" returntype="string" output="false">		<cfreturn variables.instance.verifiedEmail />	</cffunction>	<cffunction name="setsystemCreationDate" access="public" returntype="void" output="false">		<cfargument name="systemCreationDate" type="string" required="true" />		<cfset variables.instance.systemCreationDate = Trim(arguments.systemCreationDate) />	</cffunction>	<cffunction name="getsystemCreationDate" access="public" returntype="string" output="false">		<cfreturn variables.instance.systemCreationDate />	</cffunction>	<cffunction name="setlastLogin" access="public" returntype="void" output="false">		<cfargument name="lastLogin" type="string" required="true" />		<cfset variables.instance.lastLogin = Trim(arguments.lastLogin) />	</cffunction>	<cffunction name="getlastLogin" access="public" returntype="string" output="false">		<cfreturn variables.instance.lastLogin />	</cffunction></cfcomponent>
